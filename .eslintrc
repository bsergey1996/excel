{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": ["error", "windows"],
    "indent": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-trailing-spaces": "off",
    "space-before-blocks": "off",
    "key-spacing": "off",
    "keyword-spacing": "off",
    "no-debugger": "off",
    "max-len": "off"
  },
  "extends": ["eslint:recommended","google"]
}